{
  "name": "book-store",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate --config ormconfig.json --name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run --config ormconfig.json",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert --config ormconfig.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/platform-socket.io": "^7.6.5",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.5",
    "aes-js": "^3.1.2",
    "aws-sdk": "^2.836.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "cron": "^1.8.2",
    "excel4node": "^1.7.2",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "read-excel-file": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "ts-mapper": "^1.2.3",
    "typeorm": "^0.2.30",
    "useragent": "^2.3.0",
    "vue-recaptcha": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/testing": "^7.6.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.7",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.17.28",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.12",
    "@types/supertest": "^2.0.10",
    "class-transformer": "^0.2.3",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typescript": "3.8.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
